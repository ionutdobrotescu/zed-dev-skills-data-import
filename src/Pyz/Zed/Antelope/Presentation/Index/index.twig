{% extends '@Gui/Layout/layout.twig' %}
{% block content %}
    <h1>Antelopes location</h1>

    <table class="table table-striped">
        <thead>
        <tr>
            <th scope="col">id</th>
            <th scope="col">Name</th>
        </tr>
        </thead>
        <tbody>
        {% for antelope in antelopes %}
            <tr>
                <th scope="row">{{ antelope.idAntelope }}</th>
                <td>{{ antelope.name }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock content %}
