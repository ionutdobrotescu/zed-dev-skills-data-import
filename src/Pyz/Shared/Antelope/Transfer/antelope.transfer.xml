<?xml version="1.0"?>
<transfers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="spryker:transfer-01"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="Antelope">
        <property name="idAntelope" type="int"/>
        <property name="name" type="string"/>
        <property name="fkAntelopeLocation" type="int"/>
        <property name="antelopeLocation" type="AntelopeLocation"/>
    </transfer>
    <transfer name="AntelopeCollection">
        <property name="antelopes" type="Antelope[]" singular="antelope"/>
    </transfer>
    <transfer name="AntelopeCollectionResponse">
        <property name="antelopes" type="Antelope[]"/>
        <property name="isSuccessful" type="bool"/>
        <property name="errorMessage" type="string"/>
    </transfer>
    <transfer name="AntelopeResponse">
        <property name="antelope" type="Antelope"/>
        <property name="isSuccessful" type="bool"/>
        <property name="errorMessage" type="string"/>
    </transfer>
    <transfer name="AntelopeCollection">
        <property name="antelopes" type="Antelope[]" singular="antelope"/>
    </transfer>
    <transfer name="AntelopeCriteria">
        <property name="name" type="string"/>
        <property name="idAntelope" type="int"/>
    </transfer>
    <transfer name="AntelopeLocation">
        <property name="idAntelopeLocation" type="int"/>
        <property name="locationName" type="string"/>
    </transfer>

    <transfer name="AntelopeLocationCriteria">
        <property name="idAntelopeLocation" type="int"/>
        <property name="locationName" type="string"/>
    </transfer>

    <transfer name="AntelopeLocationResponse">
        <property name="antelopeLocation" type="AntelopeLocation"/>
        <property name="isSuccessful" type="bool"/>
        <property name="errorMessage" type="string"/>
    </transfer>
    <transfer name="AntelopeLocationCollectionResponse">
        <property name="antelopeLocations" type="AntelopeLocation[]"/>
        <property name="isSuccessful" type="bool"/>
        <property name="errorMessage" type="string"/>
    </transfer>


    <transfer name="AntelopeLocationCollection">
        <property name="antelopeLocations" type="AntelopeLocation[]" singular="antelopeLocation"/>
        <property name="pagination" type="Pagination"/>
    </transfer>
    <transfer name="AntelopeCollection">
        <property name="antelopes" type="Antelope[]" singular="antelope"/>
        <property name="pagination" type="Pagination"/>
    </transfer>
    <transfer name="Pagination">
        <property name="limit" type="int"/>
        <property name="offset" type="int"/>
        <property name="page" type="int"/>
        <property name="maxPerPage" type="int"/>
        <property name="nbResults" type="int"/>
        <property name="firstIndex" type="int"/>
        <property name="lastIndex" type="int"/>
        <property name="firstPage" type="int"/>
        <property name="lastPage" type="int"/>
        <property name="nextPage" type="int"/>
        <property name="previousPage" type="int"/>

    </transfer>
    <transfer name="AntelopeCondition">
        <property name="antelopeIds" singular="antelopeId" type="int[]"/>
        <property name="idAntelope" type="int"/>
        <property name="name" type="string"/>
        <property name="idLocation" type="int"/>
    </transfer>
    <transfer name="AntelopeLocationCondition">
        <property name="antelopeLocationsIds" singular="antelopeLocationId" type="int[]"/>
        <property name="name" type="string"/>
        <property name="idAntelopeLocation" type="int"/>
    </transfer>
    <transfer name="AntelopeCriteria">
        <property name="antelopeConditions" type="AntelopeCondition"/>
        <property name="sortCollection" type="Sort[]"/>
        <property name="pagination" type="Pagination"/>
    </transfer>
    <transfer name="AntelopeLocationCriteria">
        <property name="antelopeLocationsConditions" type="AntelopeLocationCondition"/>
        <property name="sortCollection" type="Sort[]"/>
        <property name="pagination" type="Pagination"/>
    </transfer>
    <transfer name="Sort">
        <property name="field" type="string"/>
        <property name="isAscending" type="bool"/>
    </transfer>
</transfers>
